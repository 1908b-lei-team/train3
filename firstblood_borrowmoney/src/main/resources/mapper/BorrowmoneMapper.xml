<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC
    "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.fh.mapper.ControlMapper">

    <select id="queryList" resultType="Control">
        select c.*,p.user_name from t_control c left join t_pay p on c.userId=p.id
    </select>

</mapper>
